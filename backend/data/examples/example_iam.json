{
  "users": [
    {
      "id": "alice",
      "name": "Alice Smith",
      "permissions": ["read:data", "write:data"],
      "roles": ["developer", "reviewer"],
      "groups": ["engineering", "security"]
    },
    {
      "id": "bob",
      "name": "Bob Jones",
      "permissions": ["read:*", "write:*", "delete:*"],
      "roles": ["admin"],
      "groups": ["engineering"]
    },
    {
      "id": "charlie",
      "name": "Charlie Brown",
      "permissions": ["read:data"],
      "roles": ["viewer"],
      "groups": ["marketing"]
    }
  ],
  "roles": [
    {
      "id": "admin",
      "permissions": ["read:*", "write:*", "delete:*", "manage:users"],
      "description": "Full system administrator"
    },
    {
      "id": "developer",
      "permissions": ["read:code", "write:code", "read:data"],
      "description": "Software developer"
    },
    {
      "id": "reviewer",
      "permissions": ["read:data", "approve:changes"],
      "description": "Code reviewer"
    },
    {
      "id": "viewer",
      "permissions": ["read:data"],
      "description": "Read-only access"
    }
  ],
  "resources": [
    {
      "id": "database-prod",
      "type": "database",
      "name": "Production Database",
      "permissions_required": ["read:data", "write:data"]
    },
    {
      "id": "api-server",
      "type": "service",
      "name": "API Server",
      "permissions_required": ["read:data"]
    },
    {
      "id": "admin-panel",
      "type": "application",
      "name": "Admin Panel",
      "permissions_required": ["read:*", "write:*"]
    }
  ],
  "access_grants": [
    {
      "user": "alice",
      "resource": "database-prod",
      "permission": "read:data",
      "granted_via": "role"
    },
    {
      "user": "bob",
      "resource": "admin-panel",
      "permission": "read:*",
      "granted_via": "role"
    },
    {
      "user": "charlie",
      "resource": "api-server",
      "permission": "read:data",
      "granted_via": "direct"
    }
  ]
}

